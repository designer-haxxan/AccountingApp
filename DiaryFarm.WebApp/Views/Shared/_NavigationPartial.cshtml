@using DiaryFarm.Models.ViewModels.UIBaseViewModels
@model NavigationViewModel
<div class="pagetitle">
<h1>@Model.Title</h1>
<nav>
    <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="@Url.Action("Analytics","Dashboard",new {area="Admin"})"><i class="bi bi-house-door"></i></a></li>
        @for (var i = 0; i < Model.Links.Count; i++)
        {
            <li class="breadcrumb-item @(i == Model.Links.Count - 1 ? "active" : "")">@Model.Links[i]</li>
        }
    </ol>
</nav>
</div>
